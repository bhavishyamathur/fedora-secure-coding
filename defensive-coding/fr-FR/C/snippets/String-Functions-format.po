#. Tag: programlisting
#, no-c-format
msgid ""
"\n"
"void log_format(const char *format, ...) __attribute__((format(printf, 1, 2)));\n"
"\n"
"void\n"
"log_format(const char *format, ...)\n"
"{\n"
"  char buf[1000];\n"
"  va_list ap;\n"
"  va_start(ap, format);\n"
"  vsnprintf(buf, sizeof(buf), format, ap);\n"
"  va_end(ap);\n"
"  log_string(buf);\n"
"}\n"
msgstr "\nvoid log_format(const char *format, ...) __attribute__((format(printf, 1, 2)));\n\nvoid\nlog_format(const char *format, ...)\n{\n  char buf[1000];\n  va_list ap;\n  va_start(ap, format);\n  vsnprintf(buf, sizeof(buf), format, ap);\n  va_end(ap);\n  log_string(buf);\n}\n"
